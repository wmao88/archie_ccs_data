  "description": "AIDE-data-converter",
  "main": "src/server.ts",
  "scripts": {
    "create-resources-dir": "mkdirp build/resources/input && mkdirp build/resources/output",
    "copy-files": "copyfiles -u 1 src/**/*.json build/",
